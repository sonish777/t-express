{
    "name": "texpress",
    "version": "1.0.0",
    "description": "TExpress is a base project setup for using ExpressJS with Typescript",
    "main": "src/index.ts",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "node dist/index.js",
        "build": "babel src --out-dir dist --extensions \".ts,.tsx\" --copy-files",
        "build:dev": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline --watch --copy-files",
        "dev": "concurrently \"npm run build:dev\" \"nodemon dist/index.js\"",
        "build:tsc": "npx tsc",
        "dev:tsc": "concurrently \"npx tsc --watch\" \"nodemon dist/index.js\"",
        "typeorm": "cross-var ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "migration:create": "npm run typeorm migration:create ./src/database/migrations/$npm_config_name",
        "migration:run": "npm run typeorm migration:run -- -d ./src/core/database/data-source.ts -t"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/sonish777/t-express.git"
    },
    "author": "Sonish Maharjan",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/sonish777/t-express/issues"
    },
    "homepage": "https://github.com/sonish777/t-express#readme",
    "dependencies": {
        "@types/config": "^3.3.0",
        "@types/sequelize": "^4.28.14",
        "config": "^3.3.9",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "ejs": "^3.1.8",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "morgan": "^1.10.0",
        "pg": "^8.8.0",
        "pg-hstore": "^2.3.4",
        "reflect-metadata": "^0.1.13",
        "typedi": "^0.10.0",
        "typeorm": "^0.3.11"
    },
    "devDependencies": {
        "@babel/cli": "^7.20.7",
        "@babel/core": "^7.20.12",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.20.13",
        "@babel/plugin-transform-parameters": "^7.20.7",
        "@babel/plugin-transform-typescript": "^7.20.13",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.18.6",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/ejs": "^3.1.1",
        "@types/express": "^4.17.15",
        "@types/express-session": "^1.17.5",
        "@types/morgan": "^1.9.4",
        "@types/node": "^18.11.18",
        "babel-plugin-transform-typescript-metadata": "^0.3.2",
        "concurrently": "^7.6.0",
        "cross-var": "^1.1.0",
        "nodemon": "^2.0.20",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.1.2",
        "typescript": "^4.9.4"
    }
}
