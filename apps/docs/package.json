{
    "name": "docs",
    "version": "0.0.1",
    "scripts": {
        "docz:dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider docz dev --config doczrc.js",
        "docz:build": "rimraf .docz && cross-env NODE_OPTIONS=--openssl-legacy-provider docz build",
        "docz:serve": "docz build && docz serve",
        "docz:deploy": "yarn deploy",
        "predeploy": "yarn docz:build",
        "deploy": "gh-pages -d .docz/public"
    },
    "dependencies": {
        "@emotion/core": "10.1.1",
        "docz": "^2.4.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "gh-pages": "^5.0.0"
    }
}
