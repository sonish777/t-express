{
    "pipeline": {
        "dev": {
            "cache": false,
            "persistent": true
        },
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**"]
        },
        "lint": { "cache": true },
        "lint:fix": { "cache": true },
        "clean": { "cache": false },
        "build:tsc": { "cache": false, "outputs": ["dist/**"] },
        "migration:create": { "cache": false },
        "migration:run": { "cache": false },
        "migration:rollback": { "cache": false },
        "seed": { "cache": false }
    }
}
